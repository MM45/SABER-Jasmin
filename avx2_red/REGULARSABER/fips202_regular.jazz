// RATE: SHAKE128; OUTLEN: SHAKE128
export fn keccak_squeezeblocks_128_128_jazz(reg u64 hp, reg u64 sp)
{
	inline int i;

	reg u8 th;

	reg u64 ts;

	stack u8[SHAKE128_RATE] h;

	stack u64[25] s;

	stack u64 shp;
	stack u64 ssp;

	for i = 0 to 25 {
		ts = (u64) [sp + 8 * i];
		s[i] = ts;
	}

	shp = hp;
	ssp = sp;

	h, s = keccak_squeezeblocks_128_128(h, s);

	hp = shp;
	sp = ssp;

	for i = 0 to SHAKE128_RATE {
		th = h[i];
		(u8) [hp + i] = th;
	}

	for i = 0 to 25 {
		ts = s[i];
		(u64) [sp + 8 * i] = ts;
	}
}

