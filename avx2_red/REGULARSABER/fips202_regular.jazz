// RATE: SHAKE128; MLEN: 32
export fn keccak_absorb_128_32_jazz(reg u64 sp, reg u64 mp) 
{
	inline int i;

	reg u8 tm;

	reg u64 ts;

	stack u8[32] m;

	stack u64[25] s;

	for i = 0 to 25 {
		ts = (u64) [sp + 8 * i];
		s[i] = ts;
	}

	for i = 0 to 32 {
		tm = (u8) [mp + i];
		m[i] = tm;
	}

	s = keccak_absorb_128_32(s, m);

	for i = 0 to 25 {
		ts = s[i];
		(u64) [sp + 8 * i] = ts;
	}
}

