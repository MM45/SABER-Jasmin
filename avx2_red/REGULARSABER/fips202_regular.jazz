export fn KeccakF1600_StatePermute_jazz(reg u64 statep)
{
	inline int i;
	
	reg u64 t;

	stack u64[25] state;
	
	stack u64 sstatep;

	for i = 0 to 25 {
		t = (u64) [statep + 8 * i];
		state[i] = t;
	}

	sstatep = statep;

	state = KeccakF1600_StatePermute(state);

	statep = sstatep;

	for i = 0 to 25 {
		t = state[i];
		(u64) [statep + 8 * i] = t;
	}
}


// INLEN: SABER_BYTES_CCA_DEC
export fn sha3_256_CCADEC_jazz(reg u64 outputp, reg u64 inputp) 
{
	inline int i;

	reg u8 t;

	stack u8[32] output;
	stack u8[SABER_BYTES_CCA_DEC] input;

	stack u64 soutputp;

	for i = 0 to SABER_BYTES_CCA_DEC {
		t = (u8) [inputp + i];
		input[i] = t;
	}

	soutputp = outputp;

	output = sha3_256_CCADEC(output, input);

	outputp = soutputp;
	
	for i = 0 to 32 {
		t = output[i];
		(u8) [outputp + i] = t;
	}	
}

