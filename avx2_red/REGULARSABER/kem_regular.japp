  b1 = (16u) bytes[(int) address_byte];
  b1 >>= 4;
  b1 &= 0x0f;
  address_byte += 1;
  b2 = (16u) bytes[(int) address_byte];
  b2 &= 0xff;
  b2 <<= 4;
  address_byte += 1;
  b3 = (16u) bytes[(int) address_byte];
  b3 &= 0x01;
  b3 <<= 12;
  b2 |= b3;
  b1 |= b2;
  address_data += 1;
  data[(int) address_data] = b1;


  b1 = (16u) bytes[(int) address_byte];
  b1 >>= 1;
  b1 &= 0x7f;
  address_byte += 1;
  b2 = (16u) bytes[(int) address_byte];
  b2 &= 0x3f;
  b2 <<= 7;
  b1 |= b2;
  address_data += 1;
