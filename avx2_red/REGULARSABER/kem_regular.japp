  address_byte += 1;
  b2 = (16u) bytes[(int) address_byte];
  b2 &= 0x7f;
  b2 <<= 6;
  b1 |= b2;
  address_data += 1;
  data[(int) address_data] = b1;


  b1 = (16u) bytes[(int) address_byte];
  b1 >>= 7;
  b1 &= 0x01;
  address_byte += 1;
  b2 = (16u) bytes[(int) address_byte];
  b2 &= 0xff;
  b2 <<= 1;
  address_byte += 1;
  b3 = (16u) bytes[(int) address_byte];
  b3 &= 0x0f;
