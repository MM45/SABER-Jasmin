    _, _, C[_i] = #ROL_64(C[_i], 10);
    _, _, C[_o] = #ROL_64(C[_o], 15);
    _, _, C[_u] = #ROL_64(C[_u], 56);

    T[_a] = C[_e];
    T[_a] = #NOT_64(T[_a]);
