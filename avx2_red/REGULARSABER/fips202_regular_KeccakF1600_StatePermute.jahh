    E[_mu] = T[_u];

    // Result s
    C[_a] = state[_bi];
    C[_e] = state[_go];
    C[_i] = state[_ku];
    C[_o] = state[_ma];
    C[_u] = state[_se];

    C[_a] ^= D[_i];
    C[_e] ^= D[_o];
    C[_i] ^= D[_u];
    C[_o] ^= D[_a];
    C[_u] ^= D[_e];

    _, _, C[_a] = #ROL_64(C[_a], 62);
    _, _, C[_e] = #ROL_64(C[_e], 55);
