    T[_a] ^= C[_a];
    E[_ma] = T[_a];

    T[_e] = C[_i];
    T[_e] = #NOT_64(T[_e]);
    T[_e] &= C[_o];
    T[_e] ^= C[_e];
    E[_me] = T[_e];

    T[_i] = C[_o];
    T[_i] = #NOT_64(T[_i]);
    T[_i] &= C[_u];
    T[_i] ^= C[_i];
    E[_mi] = T[_i];

    T[_o] = C[_u];
    T[_o] = #NOT_64(T[_o]);
    T[_o] &= C[_a];
    T[_o] ^= C[_o];
    E[_mo] = T[_o];

    T[_u] = C[_a];
