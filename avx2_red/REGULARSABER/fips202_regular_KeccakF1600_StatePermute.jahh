    T[_a] = C[_e];
    T[_a] = #NOT_64(T[_a]);
    T[_a] &= C[_i];
    T[_a] ^= C[_a];
    round += 1;
    T[_a] ^= constptr[(int) round];
    state[_ba] = T[_a];

    T[_e] = C[_i];
    T[_e] = #NOT_64(T[_e]);
    T[_e] &= C[_o];
    T[_e] ^= C[_e];
    state[_be] = T[_e];

    T[_i] = C[_o];
    T[_i] = #NOT_64(T[_i]);
    T[_i] &= C[_u];
    T[_i] ^= C[_i];
    state[_bi] = T[_i];

    T[_o] = C[_u];
    T[_o] = #NOT_64(T[_o]);
    T[_o] &= C[_a];
    T[_o] ^= C[_o];
    state[_bo] = T[_o];

    T[_u] = C[_a];
    T[_u] = #NOT_64(T[_u]);
    T[_u] &= C[_e];
    T[_u] ^= C[_u];
    state[_bu] = T[_u];

    // Result g
    C[_a] = E[_bo];
    C[_e] = E[_gu];
    C[_i] = E[_ka];
